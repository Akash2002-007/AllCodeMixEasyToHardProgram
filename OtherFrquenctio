package com.dsa.Topc;

public class OtherFrquenctio {
	    public static void main(String[] args) {

	        int num = 9598;

	        for(int i=0;i<=9;i++) {

	            int temp = num;
	            int count = 0;

	            while(temp > 0) {

	                if(temp % 10 == i)
	                    count++;

	                temp = temp / 10;
	            }

	            if(count > 0)
	                System.out.println(i + " -> " + count);
	        }
	    }
	}

