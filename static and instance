package com.nit.nito;

public class Test8 {
	int a=89;
    static  int b=20; 
	public Test8(int a) {
		super();
		this.a = a;
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public static int getB() {
		return b;
	}
	public static void setB(int b) {
		Test8.b = b;
	}
	public static void main(String[] args) {
		Test8 s=new Test8(89);
		System.out.println(s.a);
		System.out.println(s.b);
		
        
	}
	
	
	
	
	
}
